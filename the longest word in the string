#include <iostream>
#include <string>

using namespace std;
int main() {
    string s,d="",g="",h="";
    getline(cin, s);
        for (auto c : s) {
            d.append(1,c);
            if (c==' '){
                a=0;
                g=d;
                d="";
            }
            if(d.size()>g.size()&&h.size()<d.size()){
                h=d;
            }
         }       
       cout << h; 
  return 0;
}
